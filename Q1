#include <msp430.h> 


/**
 * main.c
 */
float b = 10.0;
int main(void)
{
	WDTCTL = WDTPW | WDTHOLD;	// stop watchdog timer
	//create two unsigned hex values
	unsigned char c = 0x01;
	unsigned char d = 0x02;

	//declare variables for store
	unsigned char e;
	unsigned char f;

	int a = 1;
	a+=1; //increment
	b-=1; //deincrement

	//and operation
	e = c&d;

	//or operation
	f = c|d;

	while(1);


	return 0;
}
